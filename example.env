##this are what are neeed for development
VITE_API_URL=localhost:8000
VITE_CHAT_URL=localhost:8080
VITE_DEVELOPMENT=true
VITE_DOMAIN=localhost

## HTTPS Configuration
## Set to 'true' to enable HTTPS in development (disabled by default)
## Requires mkcert certificates (localhost+3.pem and localhost+3-key.pem) or will fallback to basicSsl
# VITE_ENABLE_HTTPS=false

## Firebase Cloud Messaging (Push Notifications)
## Get from: Firebase Console > Project Settings > Cloud Messaging > Web Push certificates
VITE_FIREBASE_VAPID_KEY=your-vapid-key-here

## Required for Docker build (npm registry authentication)
FONTAWESOME_PACKAGE_TOKEN=your-fontawesome-token-here
BRYNTUM_USERNAME=your-email..domain.com
BRYNTUM_PASSWORD=trial
# Note: For Bryntum trial accounts, replace @ with .. in your email
# Example: user@gmail.com becomes user..gmail.com